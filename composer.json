{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "akika/laravel-mojo": "dev-main",
        "akika/laravel-mpesa": "dev-dev",
        "akika/laravel-mwaloni": "dev-main",
        "akika/laravel-ncba": "dev-main",
        "akika/laravel-coop": "dev-main",
        "akika/laravel-weevo": "dev-main",
        "laravel/framework": "^11.31",
        "laravel/tinker": "^2.9"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.1",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Akika\\LaravelNcba\\": "packages/akika/laravel-ncba/src/",
            "Akika\\LaravelMwaloni\\": "packages/akika/laravel-mwaloni/src/",
            "Akika\\LaravelMojo\\": "packages/akika/laravel-mojo/src/",
            "Akika\\LaravelCoop\\": "packages/akika/laravel-coop/src/",
            "Akika\\LaravelMpesa\\": "packages/akika/laravel-mpesa/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "packages/akika/laravel-mpesa"
        },
        {
            "type": "path",
            "url": "packages/akika/laravel-ncba",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "packages/akika/laravel-mwaloni",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "packages/akika/laravel-mojo",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "packages/akika/laravel-coop",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "packages/akika/laravel-weevo",
            "options": {
                "symlink": true
            }
        }
    ]
}
